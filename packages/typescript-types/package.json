{
  "name": "@shepherdorg/typescript-types",
  "version": "0.0.1",
  "description": "Types used across shepherd libraries",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash": "4.17.4",
    "md5-file": "3.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "chai": "^4.2.0",
    "expect.js": "0.3.1",
    "mocha": "^6.2.1",
    "should": "11.1.1",
    "typescript": "^3.6.3",
    "ts-node": "^8.4.1"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --exit --require ts-node/register --recursive './**/*.spec.ts'",
    "tdd": "mocha --exit -R dot --require ts-node/register --recursive --watch-extensions ts,js,json --watch './**/*.spec.ts'",
    "prepublishOnly": "yarn install --frozen-lockfile && npm run build && npm run test"
  }
}
